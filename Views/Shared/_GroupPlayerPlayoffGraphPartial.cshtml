@model List<string>
@if (Model.Count != 0)
{
    <table class="table table-striped bg-dark" style="width:20%; margin: 0 50px 0 0; border: 0 0 0 0">
        <thead>
            <tr>
                <th>
                    Nr
                </th>
                <th>
                    Group @ViewData["GroupName"]
                </th>

            </tr>
        </thead>
        <tbody>
            @{
                int counter = 1;
            }
            @foreach (var groupPlayer in Model)
                {
                <tr>
                    <td width="3%">
                        @counter
                    </td>
                    <td width="20%">
                        @groupPlayer
                    </td>
                </tr>
                    counter++;
                }
        </tbody>
    </table>
}
